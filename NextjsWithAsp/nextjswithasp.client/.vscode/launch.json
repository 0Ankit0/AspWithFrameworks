{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Next.js",
      "program": "${workspaceFolder}/node_modules/.bin/next",
      "args": [ "dev" ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": null,
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
